<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.prince.demo.dao.UserDao">
    <!-- 查询用户信息列表 -->
    <select id="queryUserList" resultType="com.prince.demo.entity.User">
        select
          user_id, user_name, gender, email, address
        from
          user
    </select>

</mapper>